<edm:ProvidedCHO rdf:about="BI 17423-77">
<dc:contributor rdf:resource="#Matsushima_Fusajir&#x14D;_&#x677E;&#x5D8B;&#x623F;&#x6B21;&#x90CE;_Ausf&#xFC;hrung"/>
<dc:contributor rdf:resource="#Utagawa_Kunisada_I._&#x521D;&#x4EE3;&#x6B4C;&#x5DDD;&#x570B;&#x8C9E;_Entwurf"/>
